@model System.Web.Mvc.PagedList<DWM.Models.Repositories.EspecialidadeMedicaViewModel>
@{
    Layout = null;
    Model.action = "../Especialidade/_ListEspecialidadeMedicaModal";
}

<div class="row">
    <div class="col-md-11" style="margin: 0px">
        @Html.Partial("_Filter", Model)
        <div id="div-list" style="padding-top: 0px">
            @Html.Partial("../Especialidade/_ListEspecialidadeMedicaModal", Model)
        </div>
    </div>
</div>


<script type="text/javascript">

    function Move(id, descricao) {
        $("#especialidade2Id").val(id);
        $("#descricao_especialidade2").val(descricao);
        $("#descricao_especialidade21").val(descricao);
        $("#btnModal").click();
    }

</script>








