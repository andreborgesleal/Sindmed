@using DWM.Models.Repositories
@using App_Dominio.Repositories
@model System.Web.Mvc.PagedList<DWM.Models.Repositories.ChamadoViewModel>
@{
    ViewBag.Title = new App_Dominio.Security.EmpresaSecurity<App_Dominio.Entidades.SecurityContext>().getEmpresa().nome;
    ViewBag.Header = "Abrir chamado";
}
<div class="row" style="margin-top: 10px">
    <div class="col-md-12">
        <div class="panel panel-primary" style="padding: -2px">
            <div class="panel-heading">
                <h2 class="panel-title">
                    Atendimento - <small class="text-white">Listagem dos chamados</small>
                </h2>
            </div>
            <div class="panel-body">
                <div id="div-list">
                    @Html.Partial("List", Model)
                </div>
            </div>
            <div class="panel-footer">
                <div class="col-lg-offset-11">
                    @Html.Partial("_defaultListButtons", new List<ButtonRepository> { new ButtonRepository() { linkText = "Incluir", actionName = "Create", controllerName = "Chamado", buttonType = "link" } })
                </div>
            </div>
        </div>
    </div>
</div>


